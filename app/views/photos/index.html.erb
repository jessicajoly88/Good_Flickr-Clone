<div class="container-fluid">

  <div class="row title-container text-center">
    <h1 class="title">Some of our favorites</h1>
  </div>


  <div id="photos-container" class="container">
    <% @photos.each do |photo| %>
      <div class="col-md-3">
        <%= image_tag photo.image.url %>
      </div>
    <% end %>
  </div>

</div>
